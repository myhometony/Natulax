<h2>プロフィール編集</h2>
<%= form_with model: @end_user, url: admin_end_user_path do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"ユーザ名"%>
  <label for="inputImage">ProfileImage</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
  <%= f.radio_button :is_active, true %><%= f.label :is_active, "有効", class: "text-active" %>
  <%= f.radio_button :is_active, false %><%= f.label :is_active, "退会済み", class: "text-invalid" %>
  <%= f.submit "変更を保存" %>
<% end %>