<h2>投稿画像編集</h2>
<%= form_with model: @post_image, url: admin_post_image_path do |f| %>
  <label for="inputTitle">Title</label>
  <%= f.text_field :title, autofocus: true, id:"inputName", placeholder:"タイトル"%>
  <label for="inputImage">Image</label>
  <%= f.file_field :image, placeholder: "投稿画像", accept: "image/*" %>
  <%= f.submit "変更を保存" %>
<% end %>