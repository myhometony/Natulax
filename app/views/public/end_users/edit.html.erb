<h2>プロフィール編集</h2>
<%= form_with model: @end_user, url: public_end_user_path do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"ユーザ名"%>
  <label for="inputImage">ProfileImage</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
  <%= f.submit "変更を保存" %>
<% end %>

<%= link_to "退会", public_end_users_unsubscribe_path %>